
--script didn't auto skidid
local fxVa = game:GetService("RunService")
local fxVa2 = game:GetService("Workspace").CurrentCamera
local fxVa3 = workspace:FindFirstChild("Junk")

local fxVa4 = Color3.fromRGB(255, 80, 10)
local fxVa5 = 3
local fxVa6 = 0.7
local fxVa7 = true
local fxVa8 = true

local function fxVa9()
    if not fxVa3 then
        fxVa3 = Instance.new("Folder")
        fxVa3.Name = "Junk"
        fxVa3.Parent = workspace
    end

    local fxVa10 = fxVa3:FindFirstChild("Football")
    if not fxVa10 then
        fxVa10 = Instance.new("Part")
        fxVa10.Name = "Football"
        fxVa10.Size = Vector3.new(2, 2, 2)
        fxVa10.Shape = Enum.PartType.Ball
        fxVa10.Position = Vector3.new(0, 5, 0)
        fxVa10.Anchored = true
        fxVa10.Parent = fxVa3
    end

    local fxVa11 = Drawing.new("Line")
    
    fxVa.RenderStepped:Connect(function()
        if fxVa10 and fxVa10:IsDescendantOf(workspace) then
            local fxVa12, fxVa13 = fxVa2:WorldToViewportPoint(fxVa10.Position)
            
            fxVa11.Thickness = fxVa5
            fxVa11.Transparency = fxVa6
            fxVa11.Color = fxVa4
            
            if fxVa8 then
                fxVa11.From = Vector2.new(fxVa2.ViewportSize.X / 2, fxVa2.ViewportSize.Y)
            end

            if fxVa13 and fxVa7 then
                fxVa11.To = Vector2.new(fxVa12.X, fxVa12.Y)
                fxVa11.Visible = true
            else
                fxVa11.Visible = false
            end
        else
            fxVa11.Visible = false
        end
    end)
end

fxVa9()
